
<div class="sign_up col-lg-6 col-md-6">
  <div class="sign_up_header">
    <h1>Регистрация</h1>
    <p>Есть аккаунт? <%= link_to('Войти', new_user_session_path) %> </p>
  </div>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name))    do |f| %>
    <%= devise_error_messages! %>

    <div class="sign_up_form">
      <div class="field">
        <h4><%= f.label :Фото %></h4>
        <%= f.file_field :avatar,class:"avatar"%>
      </div>
      <br>
      <div class="field">
        <%= f.label :Имя %><br />
        <%= f.text_field :name,class:"input"%>
      </div>

      <div class="field">
        <%= f.label "Обо мне" %><br />
        <%= f.text_field :about,class:"input"%>
      </div>

      <div class="field">
        <%= f.label "Контактный телефон" %><br />
        <%= f.text_field :contact,class:"input"%>
      </div>

      <div class="field">
        <%= f.label "Почта" %><br />
        <%= f.email_field :email,class:"input" %>
      </div>

      <div class="field">
        <%= f.label :Пароль %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off",class:"input" %>
      </div>

      <div class="field">
        <%= f.label "Повторите пароль" %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" ,class:"input"%>
      </div>


      <%= f.radio_button :check, true %> Company <br/>
      <%= f.radio_button :check, false %> User <br>


      <div class="actions">
        <%= f.submit "Зарегистрироваться",class:"sign_up_button1" %>
      </div>
      <br>
    <% end %>
      <p>delete voiti link</p>
    <%= render   "devise/shared/links" %>

  </div>
</div>

